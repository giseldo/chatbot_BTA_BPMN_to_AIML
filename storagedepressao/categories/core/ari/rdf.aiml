<aiml version="2.0">

    <!-- Algo eh um Algo -->
    <category>
        <pattern>* EH UM *</pattern>
        <template>
            <addtriple>
                <subj>
                    <star index="1"/>
                </subj>
                <pred>EH UM</pred>
                <obj>
                    <star index="3"/>
                </obj>
            </addtriple>
            Entendido.
        </template>
    </category>

    <!-- Algo eh uma Algo -->
    <category>
        <pattern>* EH UMA *</pattern>
        <template>
            <addtriple>
                <subj>
                    <lowercase>
                        <star index="1"/>
                    </lowercase>
                </subj>
                <pred>EH UMA</pred>
                <obj>
                    <lowercase>
                        <star index="3"/>
                    </lowercase>
                </obj>
            </addtriple>
            Entendido.
        </template>
    </category>

    <!--  tudo  voce sabe -->
    <category>
        <pattern># tudo * voce sabe #</pattern>
        <template>
            <lowercase>
            <select>
                <vars>?x ?z ?y</vars>
                <q>
                    <subj>?x</subj>
                    <pred>?z</pred>
                    <obj>?y</obj>
                </q>
            </select>
            </lowercase>
            Isto é tudo o que aprendi.
        </template>
    </category>

    <!-- o que voce sabe sobre -->
    <category>
        <pattern>o que voce sabe sobre *</pattern>
        <template>
            <lowercase>
            <select>
                <vars>?x ?y</vars>
                <q>
                    <subj>?x</subj>
                    <pred>?y</pred>
                    <obj>
                        <star index="1"/>
                    </obj>
                </q>
            </select>
            </lowercase>
            <lowercase>
            <select>
                <vars>?xx ?yy</vars>
                <q>
                    <subj><star index="1"/></subj>
                    <pred>?xx</pred>
                    <obj>?yy</obj>
                </q>
            </select>
            </lowercase>
        </template>
    </category>

    <!-- quem sao os  -->
    <category>
        <pattern>QUEM SÃO OS *</pattern>
        <template>
            <srai>QUEM SAO OS *</srai>
        </template>
    </category>

    <category>
        <pattern>QUEM SAO OS *</pattern>
        <template>
            <think>
                <set var="tuples">
                    <select>
                        <vars>?x</vars>
                        <q>
                            <subj>?x</subj>
                            <pred>EH UM</pred>
                            <obj>
                                <star index="1"/>
                            </obj>
                        </q>
                    </select>
                </set>
            </think>
            Os
            <star/>
            são:
            <lowercase>
            <get var="?x">
                <tuple>
                    <get var="tuples"/>
                </tuple>
            </get>
            </lowercase>
        </template>
    </category>

</aiml>